{
	"name": "ask-me",
	"displayName": "Ask me",
	"description": "Ask me about code fundamentals",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ask-me.start",
		"onCommand:ask-me.python",
		"onCommand:ask-me.javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ask-me.start",
				"title": "Auto",
				"category": "Ask me"
			},
			{
				"command": "ask-me.python",
				"title": "Python",
				"category": "Ask me"
			},
			{
				"command": "ask-me.javascript",
				"title": "Javascript",
				"category": "Ask me"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/markdown-it": "^10.0.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"graphql": "^15.1.0",
		"graphql-tag": "^2.10.3",
		"highlight.js": "^10.1.1",
		"markdown-it": "^11.0.0"
	}
}
